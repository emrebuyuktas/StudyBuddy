@page "/userlist"
@inherits StudyBuddy.WebUi.Pages.PageProcess.UserListProcess_razor

@if (userList != null)
{
    <table class="table table-hover table-sm">
        <thead>
        <tr>
            
            <th scope="col">User Name</th>
            <th scope="col">Email</th>
            
        </tr>
        </thead>
         <tbody>
            @foreach (var user in userList)
            {
                <tr>
                    <td>@user.userName</td>
                    <td>@user.email</td>
                </tr>
            }
            </tbody>
    </table>
   
    
}
